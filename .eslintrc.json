{
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "env": {
    "amd": true,
    "browser": false,
    "mocha": false,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "semi-spacing": [2, { "before": false, "after": true }]
  }
}
